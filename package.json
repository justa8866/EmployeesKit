{
  "name": "employeeskit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "tsc --noEmit && eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "netlify-deploy-cli": "netlify deploy",
    "netlify-deploy-cli-prod": "netlify deploy --prod",
    "precommit": "lint-staged && npm run lint",
    "prepare": "husky install && chmod ug+x .husky/*",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "preview": "vite preview",
    "sort-package-json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@mui/styled-engine-sc": "^5.11.11",
    "@mui/system": "^5.11.12",
    "@mui/x-date-pickers": "^6.0.1",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "firebase": "^9.17.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-router-dom": "^6.8.1",
    "react-toastify": "^9.1.1",
    "sort-by": "^0.0.2",
    "styled-components": "^5.3.8",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "netlify-cli": "^13.1.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}
